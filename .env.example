# Example environment variables for FamApp
# Copy this file to .env and fill in your actual values

# Google Maps API Key
# Get your API key from: https://console.cloud.google.com/google/maps-apis
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Firebase Configuration (if using Firebase)
# Get these values from your Firebase project settings
VITE_FIREBASE_API_KEY=your_firebase_api_key_here
VITE_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your_project_id
VITE_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=123456789
VITE_FIREBASE_APP_ID=1:123456789:web:abcdef123456

# Google Analytics (if using)
VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX

# App Configuration
VITE_APP_NAME=FamApp
VITE_APP_VERSION=1.0.0
VITE_APP_DESCRIPTION="Family Travel Made Simple"

# API URLs (for production deployment)
VITE_API_BASE_URL=https://api.famapp.com
VITE_APP_URL=https://famapp.com

# Note: NODE_ENV is automatically set by Vite based on the command used