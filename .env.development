# Development environment variables for FamApp

# Google Maps API Key (Development)
VITE_GOOGLE_MAPS_API_KEY=

# Firebase Configuration (Development)
VITE_FIREBASE_API_KEY=
VITE_FIREBASE_AUTH_DOMAIN=
VITE_FIREBASE_PROJECT_ID=
VITE_FIREBASE_STORAGE_BUCKET=
VITE_FIREBASE_MESSAGING_SENDER_ID=
VITE_FIREBASE_APP_ID=

# Google Analytics (Development - optional)
VITE_GA_MEASUREMENT_ID=

# App Configuration
VITE_APP_NAME=FamApp (Dev)
VITE_APP_VERSION=1.0.0-dev
VITE_APP_DESCRIPTION="Family Travel Made Simple - Development"

# API URLs (Development)
VITE_API_BASE_URL=http://localhost:3001
VITE_APP_URL=http://localhost:5173

# Note: NODE_ENV is automatically set by Vite based on the command used